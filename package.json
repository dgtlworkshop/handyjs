{
  "name": "dgtl-handyjs",
  "version": "1.0.0",
  "description": "A collection of handy functions and reference types used across DGTL Javascript projects",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node scripts/cleanup.mjs esm && tsc -p ./tsconfig.json",
    "prepack": "npm run build && publint",
    "watch": "tsc -w",
    "check": "tsc --noEmit",
    "format": "prettier ."
  },
  "author": {
    "name": "Matthew Conto",
    "email": "matthew@dgtlworkshop.com"
  },
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.0.0",
    "publint": "^0.1.16",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}